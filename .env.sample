# .env.sample
# Copy this file to `.env` and replace the placeholder values with your real keys.
# The `.env` file should NOT be committed to version control.

# Google Places API key used by shop_analysis.py
GOOGLE_API_KEY=YOUR_GOOGLE_PLACES_API_KEY_HERE

# Optional: credentials for SMS outreach via Twilio (if you add that feature later)
TWILIO_ACCOUNT_SID=YOUR_TWILIO_ACCOUNT_SID_HERE
TWILIO_AUTH_TOKEN=YOUR_TWILIO_AUTH_TOKEN_HERE
TWILIO_FROM_PHONE=YOUR_TWILIO_PHONE_NUMBER_HERE

# Optional: credentials for email outreach via SendGrid or a similar service
SENDGRID_API_KEY=YOUR_SENDGRID_API_KEY_HERE
